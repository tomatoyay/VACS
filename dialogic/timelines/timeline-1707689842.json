			"character": "character-1696611985.json",
			"event_id": "dialogic_010",
			"options": [

			],
			"portrait": "Thinking",
			"question": "I heard that the one we’re sending it to might have it out for us."
		},
		{
			"choice": "Where did you hear that?",
			"condition": "",
			"definition": "",
			"event_id": "dialogic_011",
			"value": ""
