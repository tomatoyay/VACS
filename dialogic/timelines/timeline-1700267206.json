			},
			"type": 0,
			"z_index": 0
		},
		{
			"character": "character-1702954318.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Do you have business with the police?"
		},
		{
			"character": "character-1696610736.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "UH!\nNo, ma’am! I think I just got lost."
		},
		{
			"character": "character-1702954318.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Detective Gou Enson."
		},
		{
			"animation": "instant_out.gd",
			"animation_length": 0,
			"change_mirror_portrait": false,
			"change_z_index": false,
