extends Control

################################################################################
##								PUBLIC
################################################################################

func open() -> void:
	show()

################################################################################
##								PRIVATE
################################################################################

func _ready() -> void:
	pass
	
	
func _on_Yes_pressed():
	get_tree().call("quit")


func _on_No_pressed():
	hide()
	get_parent().get_parent().get_parent().resume_game()
